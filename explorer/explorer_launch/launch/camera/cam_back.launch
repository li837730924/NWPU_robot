<launch>
<node name="camera_back" pkg="usb_cam" type="usb_cam_node" >
	<param name="video_device" value="/dev/video3"/>
	<param name="image_width" value="320"/>
	<param name="image_height" value="240"/>
	<param name="pixel_format" value="yuyv"/>
	<param name="camera_frame_id" value="camera_right"/>
	<param name="camera_info_url" value="file:///$(find explorer_launch)/launch/camera/yaml/video1.yaml"/>
	<param name="io_method" value="mmap"/>
	<param name="framerate" value="30"/>
	<param name="autofocus" value="true"/>
</node>


