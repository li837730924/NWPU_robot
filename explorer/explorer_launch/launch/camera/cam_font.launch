<launch>

<node name="camera_font" pkg="usb_cam" type="usb_cam_node" respawn="true">
<param name="video_device" value="/dev/video4"/>
<param name="image_width" value="320"/>
<param name="image_height" value="240"/>
<param name="pixel_format" value="yuyv"/>
<param name="camera_frame_id" value="camera_font"/>
<param name="camera_info_url" value="file:///$(find explorer_launch)/launch/camera/yaml/video1.yaml"/>
<param name="io_method" value="mmap"/>
<param name="framerate" value="15"/>
<param name="autofocus" value="true"/>
<remap from="/camera_bak/image_raw" to="/camera2/rgb/image_raw" />
</node>

<node name="camera_back" pkg="usb_cam" type="usb_cam_node" respawn="true">
<param name="video_device" value="/dev/video5"/>
<param name="image_width" value="320"/>
<param name="image_height" value="240"/>
<param name="pixel_format" value="yuyv"/>
<param name="camera_frame_id" value="camera_back"/>
<param name="camera_info_url" value="file:///$(find explorer_launch)/launch/camera/yaml/video1.yaml"/>
<param name="io_method" value="mmap"/>
<param name="framerate" value="15"/>
<param name="autofocus" value="true"/>
<remap from="/camera_bak/image_raw" to="/camera2/rgb/image_raw" />
</node>

<arg name="camera_name_front" default="/camera_font_warp/image_raw" />
<node pkg="image_warp" name="image_warp_right" type="image_warp">
     <param name="camera_name" value="$(arg camera_name_front)" />
     <param name="img_name" value="/camera_font/image_raw" />
</node>


